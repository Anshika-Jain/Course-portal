    <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
        <div class="row">   
    <!--Information Node-->
    <div id="info-blank">
        <form id="infoblank" name="infoblank">
        <div class="infoheader">   
            <p>Add the Course title</p>
            <input type="text" placeholder="Course Title" id="coursename" name="coursename">
            <p>Choose the category</p>
                <select id="category-dd"> 
                    <option value="Development">Development</option> 
                    <option value="IT">IT & Software</option>
                    <option value="Academics">Academics</option>
                    <option value="Marketing">Marketing</option>
                    <option value="Business">Business</option> 
                    <option value="Lifestyle">Lifestyle</option> 
                    <option value="Health">Health & Fitness</option> 
                    <option value="Music">Music</option> 
                </select>
            
            <br>    
            <p>Add the Course Code</p>
            <input type="text" placeholder="Course Code" id="code" name="code">
            <br>
            <p>Choose the difficulty level for the course</p>
                <select id="diffi-dd"> 
                    <option value="Easy">Easy</option> 
                    <option value="Intermediate">Intermediate</option> 
                    <option value="Difficult">Difficult</option> 
                     
                </select>
            <br><br>
        </div> 
        <div class="infofooter">
           <input type="button" value="Next" class="next" onclick="Saveinfo();">
            <input type="button" value="Cancel" class="cancel" onclick="cancel();">
        </div>    
        </form>
   </div>

   <!--About the course Node-->
   
      
   <div class="container">
    <div id="basic-info" style="visibility: hidden;" >
      <h5 id="CName" style=" font-weight: 100;"></h5>
      <h5 id="Ccode" style=" font-weight: 100;"></h5>
   </div>
    <div class="row">
      

      <div class="col-sm" id ="section"style="padding-right: 10%; visibility: hidden;">
        <h4 class="heading1">Sections<i class="fa fa-plus"></i></h4>
    <input type="text" placeholder="Add a New Section">

    <ul>
        <li><span><i class="fa fa-trash"></i></span> About the Course</li>
        <li><span><i class="fa fa-trash"></i></span> Section 1</li>
        <li><span><i class="fa fa-trash"></i></span> Section 2</li>
    </ul>
      </div>
      
      <div class="col-sm"  id="node-blank" style="visibility: hidden;">
       
    <h1 class="heading1">About the course</h1>
    <div class="node">
        <form>
          <textarea placeholder="Add the description of course here." id="about" name="about"  rows="13" cols="190"></textarea>
      </form>
        <div class="nodefooter">
        <input type="button" value="Reset" class="reset" onclick="Resetinfo();">
        <input type="button" value="Save" class="savenode" onclick="Savenode();">
        </div>
        
    </div>
      </div>
     
    </div>
  </div>
   
  
